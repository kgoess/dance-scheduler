<% MACRO make_link(path, anchor) BLOCK -%>
<%
    IF is_homepage;
        IF path == "";
            link = anchor;
        ELSE;
            link = request.uri_for(path) _ anchor;
        END;
    ELSE;
        IF path == "";
            IF settings.environment == "development";
                link = request.uri_for("/unearth") _ anchor;
            ELSE;
                link = 'https://bacds.org' _ anchor;
            END;
        ELSE;
            link = request.uri_for(path) _ anchor;
        END;
    END -%>
